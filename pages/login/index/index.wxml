<!--pages/login/index/index.wxml-->
<view class='login_index'>
  <image src='/image/login/login_bg.png' class='backgroundImg'></image> <!-- -->
  <view class="logincontiner" style="text-align: center">
    <image class='temp_login_logo' src='/image/login/login_logo.png'></image>
    <rich-text  class='appName'>小龟快跑 商家版</rich-text>
    <view class='loginBtn'>
      <button open-type="getUserInfo" bindgetuserinfo='getUserInfo'>
        <image src='/image/login/wx_icon.png'></image>
        <text>授权登录</text>
      </button>
    </view>
  </view>
</view>
